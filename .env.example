# Supabase Configuration (Pre-configured)
VITE_SUPABASE_URL=your_supabase_url
VITE_SUPABASE_SUPABASE_ANON_KEY=your_supabase_anon_key

# Google Services (Required for GMB & Places features)
GOOGLE_SERVICE_ACCOUNT_JSON=your_service_account_json
GOOGLE_PLACES_API_KEY=your_places_api_key
GMB_OAUTH_CLIENT_ID=your_gmb_oauth_client_id
GMB_OAUTH_CLIENT_SECRET=your_gmb_oauth_client_secret

# AI Services (Required)
GEMINI_API_KEY=your_gemini_api_key

# Image Generation (Choose one provider)
IMAGE_PROVIDER=stable-diffusion
IMAGE_PROVIDER_API_KEY=your_image_provider_api_key

# Blogger Integration (Optional)
BLOGGER_CLIENT_ID=your_blogger_client_id
BLOGGER_CLIENT_SECRET=your_blogger_client_secret

# Payment Gateways
# Razorpay (India)
RAZORPAY_KEY_ID=your_razorpay_key_id
RAZORPAY_KEY_SECRET=your_razorpay_key_secret
RAZORPAY_WEBHOOK_SECRET=your_razorpay_webhook_secret

# Stripe (International)
STRIPE_SECRET_KEY=your_stripe_secret_key
STRIPE_WEBHOOK_SECRET=your_stripe_webhook_secret

# PayPal (International)
PAYPAL_CLIENT_ID=your_paypal_client_id
PAYPAL_CLIENT_SECRET=your_paypal_client_secret
PAYPAL_WEBHOOK_ID=your_paypal_webhook_id

# Application
APP_BASE_URL=http://localhost:5173
JWT_SECRET=your_jwt_secret
